  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
